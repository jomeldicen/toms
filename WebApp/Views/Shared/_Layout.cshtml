<!DOCTYPE html>
<html ng-app="app" ng-controller="global">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.PageTitle - @ViewBag.Title</title>
    <meta name="description" content="@ViewBag.MetaDescription">
    <meta name="keywords" content="@ViewBag.MetaKeyword">
    <meta name="author" content="@ViewBag.SiteAuthor">

    <link href="~/Content/ProjectFile/jquery.dataTables.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- pace-progress -->
    <link rel="stylesheet" href="~/Content/plugins/pace-progress/themes/blue/pace-theme-flat-top.css">
    <!-- iCheck for checkboxes and radio inputs -->
    <link rel="stylesheet" href="~/Content/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- AngularJS Material Design -->
    <link rel="stylesheet" href="~/Content/dist/css/angular-material.min.css">
    @RenderSection("csss", required: false)
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/dist/css/adminlte.min.css">
    <link rel="stylesheet" href="~/Content/dist/css/custom.css">
    <link rel="stylesheet" href="~/Content/dist/css/spinner.css">

    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <style>
        .form-group .invalid-feedback {
            display: none;
        }

        .form-group.has-error .invalid-feedback {
            display: block;
        }
    </style>
</head>
<body class="hold-transition skin-blue sidebar-mini @ViewBag.BodySmallText " ng-init="GetBasicData()" ng-cloak>
    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-dark @ViewBag.NavbarColorVariant">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#"><i class="fas fa-bars"></i></a>
                </li>
            </ul>

            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                @*<li class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#">
                        <i class="far fa-bell"></i>
                        <span class="badge badge-warning navbar-badge">15</span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-lg dropdown-menu-right">
                        <span class="dropdown-item dropdown-header">15 Notifications</span>
                        <div class="dropdown-divider"></div>
                        <a href="#" class="dropdown-item">
                            <i class="fas fa-envelope mr-2"></i> 4 new messages
                            <span class="float-right text-muted text-sm">3 mins</span>
                        </a>
                        <div class="dropdown-divider"></div>
                        <a href="#" class="dropdown-item">
                            <i class="fas fa-users mr-2"></i> 8 friend requests
                            <span class="float-right text-muted text-sm">12 hours</span>
                        </a>
                        <div class="dropdown-divider"></div>
                        <a href="#" class="dropdown-item">
                            <i class="fas fa-file mr-2"></i> 3 new reports
                            <span class="float-right text-muted text-sm">2 days</span>
                        </a>
                        <div class="dropdown-divider"></div>
                        <a href="#" class="dropdown-item dropdown-footer">See All Notifications</a>
                    </div>
                </li>*@
                <!-- Notifications Dropdown Menu -->
                <li class="nav-item dropdown user user-menu">
                    <a href="#" class="nav-link dropdown-toggle" data-toggle="dropdown">
                        <img ng-if="Photo == null" src="~/Content/ProjectFile/img/avatar5.png" class="user-image img-circle elevation-2" alt="User Image">
                        <img ng-if="Photo != null" ng-src="{{Photo}}" class="user-image img-circle elevation-2" alt="User Image">
                        <span class="d-none d-md-block float-right" ng-bind="GetName"></span>
                    </a>
                    <ul class="dropdown-menu dropdown-menu-lg dropdown-menu-right">
                        <!-- User image -->
                        <li class="user-header bg-gray-dark">
                            <img ng-if="Photo == null" src="~/Content/ProjectFile/img/avatar5.png" class="img-circle elevation-2" alt="User Image">
                            <img ng-if="Photo != null" ng-src="{{Photo}}" class="img-circle elevation-2" alt="User Image">
                            <p class="mb-0" ng-bind="GetName"></p>
                            <p class="mt-0 text-xs" ng-bind="GetRole"></p>
                        </li>
                        <!-- Menu Body -->
                        <!-- Menu Footer-->
                        <li class="user-footer">
                            <div class="pull-left" style="float: left!important;">
                                <a href="Profile" class="btn btn-secondary btn-flat">Profile</a>
                            </div>
                            <div class="pull-right" style="float: right!important;">
                                <a href="#" class="btn btn-secondary btn-flat" ng-click="logout()">Sign out</a>
                            </div>
                        </li>
                    </ul>
                </li>
            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container @ViewBag.LightSidebarVariant-->
        <aside class="main-sidebar elevation-1 @ViewBag.DarkSidebarVariant">
            <!-- Brand Logo -->
            <a href="/Admin/Index" class="brand-link @ViewBag.BrandLogoVariant">
                <img src="~/Content/images/logo.png" alt="FLI Logo" class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">FLI - Project @ViewBag.SiteCode</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img ng-if="Photo == null" src="~/Content/ProjectFile/img/avatar5.png" class="img-circle elevation-2" alt="User Image">
                        <img ng-if="Photo != null" ng-src="{{Photo}}" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="Profile" class="d-block"> <span class="d-none d-md-block" ng-bind="GetName"></span></a>
                        <p class="text-xs mb-0 text-white-50"><span class="d-none d-md-block" ng-bind="GetRole"></span></p>
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2" ng-init="GetMenuBasicData()">
                    <ul class="nav nav-pills nav-sidebar flex-column nav-legacy nav-child-indent nav-compact" data-widget="treeview" role="menu" data-accordion="false" ng-bind-html="MenuHtml">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <div class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1 class="m-0 text-dark">@ViewBag.Title</h1>
                        </div><!-- /.col -->
                        <div class="col-sm-6">
                            <ol class="mt-1 breadcrumb float-sm-right text-xs">
                                <li class="breadcrumb-item"><a href="/Admin/Index"><i class="fas fa-tachometer-alt"></i> Home</a></li>
                                <li class="breadcrumb-item active">@ViewBag.Title</li>
                            </ol>
                        </div><!-- /.col -->
                    </div><!-- /.row -->
                </div><!-- /.container-fluid -->
            </div>
            <!-- /.content-header -->
            <!-- Main content -->
            @RenderBody()
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <!-- Main Footer -->
        <footer class="main-footer">
            <!-- Default to the left -->
            <strong>Copyright &copy; 2020-@DateTime.Now.Year <a href="@ViewBag.CorporateEmail">@ViewBag.Company</a>.</strong> All rights reserved.
        </footer>
    </div>

    <!-- jQuery -->
    <script src="~/Content/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/Content/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- pace-progress -->
    <script src="~/Content/plugins/pace-progress/pace.js"></script>
    <!-- Summernote -->
    <script src="~/Content/plugins/summernote/summernote-bs4.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/dist/js/adminlte.min.js"></script>

    <script src="~/Scripts/AngularJS/1.8.0/angular.min.js"></script>
    <script src="~/Scripts/AngularJS/1.8.0/angular-sanitize.min.js"></script>
    <script src="~/Scripts/AngularJS/1.8.0/angular-animate.min.js"></script>
    <script src="~/Scripts/AngularJS/1.8.0/angular-route.min.js"></script>
    <script src="~/Scripts/AngularJS/1.8.0/angular-aria.min.js"></script>
    <script src="~/Scripts/AngularJS/1.8.0/angular-messages.min.js"></script>
    <script src="~/Scripts/AngularJS/1.8.0/angularjs-dropdown-multiselect.min.js"></script>
    <script src="~/Scripts/AngularJS/1.8.0/svg-assets-cache.js"></script>
    <script src="~/Scripts/AngularJS/1.8.0/angular-material.min.js"></script>
    <script src="~/Scripts/AngularJS/1.8.0/angular-summernote.js"></script>
    <script src="~/Scripts/ui-bootstrap-tpls-3.0.2.min.js"></script>
    <script src="~/Scripts/ProjectScriptFile/sweetalert.min.js"></script>
    <script src="~/Scripts/ProjectScriptFile/AppAdmin.js"></script>
    <script src="~/Content/ProjectFile/jquery.dataTables.min.js"></script>
    <script src="~/Content/ProjectFile/aungular-datatable.js"></script>
    <script src="~/Scripts/ProjectScriptFile/Global.js"></script>

    @*<script src="~/Scripts/jquery.signalR-2.4.1.min.js"></script>
    <script src="~/signalr/js"></script>
    <script src="~/Scripts/SignalR/js"></script>*@

    @RenderSection("scripts", required: false)

</body>
</html>
