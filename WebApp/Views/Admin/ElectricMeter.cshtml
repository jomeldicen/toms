                                                                    <div class="col-md-7">
                                                                        <div class="input-group mb-0">
                                                                            <input type="text" name="MeralcoReceiptDate" id="MeralcoReceiptDate" class="form-control form-control-sm" @*onkeydown="return false"*@ alt-input-formats="altInputFormats"
                                                                                   uib-datepicker-popup="{{dateFormat}}" ng-model="ElectricMeter.MeralcoReceiptDate" is-open="popup3.opened" datepicker-options="dateReceiptMeralco"
