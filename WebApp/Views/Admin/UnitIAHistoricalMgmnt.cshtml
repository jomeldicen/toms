
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div ng-controller="UnitIAHistoricalMgmnt" ng-init="init('@ViewBag.PageUrl', '@ViewBag.Variable')">
    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-3" ng-init="GetSearchData()">
                    <div class="card">
                        <div class="card-header card-gray card-outline">
                            <h3 class="card-title">
                                <i class="fas fa-bars mr-1"></i>Search
                            </h3>
                        </div>
                        <!-- /.card-header -->
                        <div class="card-body">
                            <form name="UnitQualification" novalidate role="form" ng-submit="$event.preventDefault()">
                                <div class="row">
                                    <div class="col-12 form-group mb-2 has-feedback">
                                        <label class="mb-1" for="SProjectCode">Project Name</label>
                                        <md-autocomplete ng-disabled="false" md-no-cache="true" md-selected-item="selectedProject" md-search-text-change="searchTextChange(searchProject, 'Project')"
                                                         md-search-text="searchProject" md-selected-item-change="selectedItemChange(item, 'Project')" md-items="item in querySearch(searchProject, 'Project')"
                                                         md-item-text="item.ProjectCodeName" md-min-length="0" placeholder="Ex. Bay Garden" input-aria-labelledby="favoriteStateLabel"
                                                         md-menu-class="autocomplete-custom-template" md-menu-container-class="custom-container">
                                            <md-item-template>
                                                <span class="item-title">
                                                    <i class="fas fa-building"></i>
                                                    <span md-highlight-text="searchProject" md-highlight-flags="i">{{item.CompanyCode}} : {{item.ProjectCode}} </span>
                                                </span>
                                                <span class="item-metadata">
                                                    <span md-highlight-text="searchProject" md-highlight-flags="i">{{item.BusinessEntity}}</span>
                                                </span>
                                            </md-item-template>
                                            <md-not-found>
                                                No states matching "{{searchProject}}" were found.
                                            </md-not-found>
                                        </md-autocomplete>
                                    </div>

                                    <div class="col-12 form-group mb-2 has-feedback">
                                        <label class="mb-1" for="SProjectLocation">Project Location</label>
                                        <textarea class="form-control form-control-sm" id="SProjectLocation" name="SProjectLocation" rows="2" ng-model="paramInfo.ProjectLocation" placeholder="Location" readonly="readonly"></textarea>
                                    </div>

                                    <div class="col-12 form-group mb-2 has-feedback">
                                        <label class="mb-1" for="SUnitCategory">Unit Category</label>
                                        <md-autocomplete ng-disabled="false" md-no-cache="true" md-selected-item="selectedUnitCategory" md-search-text-change="searchTextChange(searchUnitCategory, 'UnitCategory')"
                                                         md-search-text="searchUnitCategory" md-selected-item-change="selectedItemChange(item, 'UnitCategory')" md-items="item in querySearch(searchUnitCategory, 'UnitCategory')"
                                                         md-item-text="item.name" md-min-length="0" placeholder="Ex. Residential Unit" input-aria-labelledby="favoriteStateLabel">
                                            <md-item-template>
                                                <span md-highlight-text="searchUnitCategory" md-highlight-flags="^i">{{item.name}}</span>
                                            </md-item-template>
                                            <md-not-found>
                                                No states matching "{{searchUnitCategory}}" were found.
                                            </md-not-found>
                                        </md-autocomplete>
                                    </div>

                                    <div class="col-12 form-group mb-2 has-feedback">
                                        <label class="mb-1" for="SUnitNos">Unit Nos</label>
                                        <md-autocomplete ng-disabled="false" md-no-cache="true" md-selected-item="selectedUnit" md-search-text-change="searchTextChange(searchUnit, 'Unit')"
                                                         md-search-text="searchUnit" md-selected-item-change="selectedItemChange(item, 'Unit')" md-items="item in querySearch(searchUnit, 'Unit')"
                                                         md-item-text="item.RefNos" md-min-length="0" placeholder="Ex. 1A" input-aria-labelledby="favoriteStateLabel">
                                            <md-item-template>
                                                <span md-highlight-text="searchUnit" md-highlight-flags="^i">{{item.RefNos}}</span>
                                            </md-item-template>
                                            <md-not-found>
                                                No states matching "{{searchUnit}}" were found.
                                            </md-not-found>
                                        </md-autocomplete>
                                    </div>

                                    <div class="col-12 form-group mb-3 has-feedback">
                                        <label class="mb-1" for="SCustomerNos">Customer Nos</label>
                                        <md-autocomplete ng-disabled="false" md-no-cache="true" md-selected-item="selectedCustomer" md-search-text-change="searchTextChange(searchCustomer, 'Customer')"
                                                         md-search-text="searchCustomer" md-selected-item-change="selectedItemChange(item, 'Customer')" md-items="item in querySearch(searchCustomer, 'Customer')"
                                                         md-item-text="item.CustomerNos" md-min-length="0" placeholder="Ex. 0000000001" input-aria-labelledby="favoriteStateLabel">
                                            <md-item-template>
                                                <span class="item-title">
                                                    <span md-highlight-text="searchCustomer" md-highlight-flags="^i">{{item.CustomerNos}}</span>
                                                    <small>- {{item.CustomerName1}}</small>
                                                </span>
                                            </md-item-template>
                                            <md-not-found>
                                                No states matching "{{searchCustomer}}" were found.
                                            </md-not-found>
                                        </md-autocomplete>
                                    </div>

                                    <div class="col-12 form-group mb-0">
                                        <button type="button" class="btn btn-default btn-sm float-right" data-dismiss="modal" ng-click="clearSearch()">
                                            <i class="fas fa-times-circle"></i> Clear
                                        </button>
                                        <button type="button" class="btn btn-secondary btn-sm float-right  mr-1" ng-click="GetUnitInspectData()">
                                            <i class="fas fa-search"></i> Search
                                        </button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>

                <div class="col-md-9">
                    <div class="loading" ng-if="loading">Loading &#8230;</div>
                    <div class="card">
                        <div class="card-header card-gray card-outline">
                            <h3 class="card-title">
                                <i class="fas fa-th mr-1"></i>Unit Details
                            </h3>
                            <div class="card-tools">
                                <button type="button" class="btn btn-tool" data-card-widget="collapse">
                                    <i class="fas fa-minus"></i>
                                </button>
                                <button type="button" class="btn btn-tool" data-card-widget="remove">
                                    <i class="fas fa-times"></i>
                                </button>
                            </div>
                        </div>
                        <!-- /.card-header -->
                        <div class="card-body">
                            <div class="row">
                                <div class="col-6">
                                    <div class="form-group row mb-0">
                                        <label for="UnitNos" class="col-sm-5 col-form-label">Unit Nos</label>
                                        <div class="col-sm-7">
                                            <input type="text" class="form-control form-control-sm" id="UnitNos" name="UnitNos" ng-model="HistoricalInfo.RefNos" placeholder="0" ng-disabled="true">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="form-group row mb-0">
                                        <label for="CustomerNos" class="col-sm-5 col-form-label">Customer Nos</label>
                                        <div class="col-sm-7">
                                            <input type="text" class="form-control form-control-sm" id="CustomerNos" name="CustomerNos" ng-model="HistoricalInfo.CustomerNos" placeholder="0" ng-disabled="true">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-6">
                                    <div class="form-group row mb-0">
                                        <label for="CompanyCode" class="col-sm-5 col-form-label font-weight-normal">Company Code</label>
                                        <div class="col-sm-7">
                                            <input type="text" class="form-control form-control-sm" id="CompanyCode" name="CompanyCode" ng-model="HistoricalInfo.CompanyCode" placeholder="" ng-disabled="true">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="form-group row mb-0">
                                        <label for="CompanyName" class="col-sm-5 col-form-label font-weight-normal">Company Name</label>
                                        <div class="col-sm-7">
                                            <input type="text" class="form-control form-control-sm" id="CompanyName" name="CompanyName" ng-model="HistoricalInfo.CompanyName" placeholder="" ng-disabled="true">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-6">
                                    <div class="form-group row mb-0">
                                        <label for="ProjectCode" class="col-sm-5 col-form-label font-weight-normal">Project Code</label>
                                        <div class="col-sm-7">
                                            <input type="text" class="form-control form-control-sm" id="ProjectCode" name="ProjectCode" ng-model="HistoricalInfo.ProjectCode" placeholder="" ng-disabled="true">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="form-group row mb-0">
                                        <label for="ProjectName" class="col-sm-5 col-form-label font-weight-normal">Project Name</label>
                                        <div class="col-sm-7">
                                            <input type="text" class="form-control form-control-sm" id="ProjectName" name="ProjectName" ng-model="HistoricalInfo.BusinessEntity" placeholder="" ng-disabled="true">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-6">
                                    <div class="form-group row mb-0">
                                        <label for="ProjectLocation" class="col-sm-5 col-form-label font-weight-normal">Project Location</label>
                                        <div class="col-sm-7">
                                            <input type="text" class="form-control form-control-sm" id="ProjectLocation" name="ProjectLocation" ng-model="HistoricalInfo.ProjectLocation" placeholder="" ng-disabled="true">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="form-group row mb-0">
                                        <label for="FloorNos" class="col-sm-5 col-form-label font-weight-normal">Floor Nos</label>
                                        <div class="col-sm-7">
                                            <input type="text" class="form-control form-control-sm" id="FloorNos" name="FloorNos" ng-model="HistoricalInfo.Phase" placeholder="" ng-disabled="true">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-6">
                                    <div class="form-group row mb-0">
                                        <label for="UnitTypeCode" class="col-sm-5 col-form-label font-weight-normal">Unit Type Code</label>
                                        <div class="col-sm-7">
                                            <input type="text" class="form-control form-control-sm" id="UnitTypeCode" name="UnitTypeCode" ng-model="HistoricalInfo.UnitType" placeholder="" ng-disabled="true">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="form-group row mb-0">
                                        <label for="UnitDescription" class="col-sm-5 col-form-label font-weight-normal">Unit Description</label>
                                        <div class="col-sm-7">
                                            <input type="text" class="form-control form-control-sm" id="UnitDescription" name="UnitDescription" ng-model="HistoricalInfo.UnitTypeDesc" placeholder="" ng-disabled="true">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-6">
                                    <div class="form-group row mb-0">
                                        <label for="UnitCategory" class="col-sm-5 col-form-label font-weight-normal">Unit Category</label>
                                        <div class="col-sm-7">
                                            <input type="text" class="form-control form-control-sm" id="UnitCategory" name="UnitCategory" ng-model="HistoricalInfo.UnitCategoryDesc" placeholder="" ng-disabled="true">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="form-group row mb-0">
                                        <label for="UnitArea" class="col-sm-5 col-form-label font-weight-normal">Unit Area (sqm)</label>
                                        <div class="col-sm-7">
                                            <input type="text" class="form-control form-control-sm" id="UnitArea" name="UnitArea" ng-model="HistoricalInfo.UnitArea" placeholder="0" ng-disabled="true">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row mb-3">
                                <div class="col-6">
                                    <div class="form-group row mb-0">
                                        <label for="CustomerName" class="col-sm-5 col-form-label">Customer Name</label>
                                        <div class="col-sm-7">
                                            <input type="text" class="form-control form-control-sm" id="CustomerName" name="CustomerName" ng-model="HistoricalInfo.CustomerName1" placeholder="" ng-disabled="true">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="form-group row mb-0">
                                        <label for="AccountType" class="col-sm-5 col-form-label font-weight-normal">Account Type</label>
                                        <div class="col-sm-7">
                                            <input type="text" class="form-control form-control-sm" id="AccountType" name="AccountType" ng-model="HistoricalInfo.AccountTypeDesc" placeholder="0" ng-disabled="true">
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row mb-0">
                                <div class="col-12">
                                    <div class="card card-gray card-tabs">
                                        <div class="card-header p-0 pt-1">
                                            <ul class="nav nav-tabs" id="custom-content-below-tab" role="tablist">
                                                <li class="nav-item">
                                                    <a class="nav-link active" id="custom-unit-tab1" data-toggle="pill" href="#custom-tab1" role="tab" aria-controls="custom-tab1" aria-selected="true">Turnover &amp; Acceptance Status</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" id="custom-notice-tab2" data-toggle="pill" href="#custom-tab2" role="tab" aria-controls="custom-tab2" aria-selected="false">Deemed Acceptance Status</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" id="custom-clients-tab3" data-toggle="pill" href="#custom-tab3" role="tab" aria-controls="custom-tab3" aria-selected="false">Clients Information</a>
                                                </li>
                                            </ul>
                                        </div>
                                        <div class="card-body">
                                            <div class="tab-content" id="custom-content-below-tabContent">
                                                <div class="tab-pane fade show active" id="custom-tab1" role="tabpanel" aria-labelledby="custom-tab1">
                                                    <form name="toacceptstatusForm" novalidate role="form">
                                                        <p class="font-weight-bold mb-2"><u>Details: Turnover Schedule &amp; Option </u></p>
                                                        <div class="row">
                                                            <div class="col-6">
                                                                <div class="form-group row mb-0">
                                                                    <label for="FinalTurnoverDate" class="col-sm-5 col-form-label font-weight-normal">Turnover Date</label>
                                                                    <div class="col-sm-7">
                                                                        <div class="input-group mb-0">
                                                                            <input type="date" class="form-control form-control-sm" id="FinalTurnoverDate" name="FinalTurnoverDate" ng-model="HistoricalInfo.FinalTurnoverDate" placeholder="mm/dd/yyyy" ng-disabled="true">
                                                                            <div class="input-group-append">
                                                                                <div class="input-group-text">
                                                                                    <span class="fas fa-calendar-alt"></span>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-6">
                                                                <div class="form-group row mb-0">
                                                                    <label for="FinalTurnoverTime" class="col-sm-5 col-form-label font-weight-normal">Turnover Time</label>
                                                                    <div class="col-sm-7">
                                                                        <div class="input-group mb-0">
                                                                            <input type="time" class="form-control form-control-sm" id="FinalTurnoverTime" name="FinalTurnoverTime" ng-model="HistoricalInfo.FinalTurnoverTime" placeholder="HH:mm TT" ng-disabled="true">
                                                                            <div class="input-group-append">
                                                                                <div class="input-group-text">
                                                                                    <span class="fas fa-clock"></span>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-6">
                                                                <div class="form-group row mb-0">
                                                                    <label for="FinalTurnoverOption" class="col-sm-5 col-form-label font-weight-normal">Turnover Option</label>
                                                                    <div class="col-sm-7">
                                                                        <input type="text" class="form-control form-control-sm" id="FinalTurnoverOption" name="FinalTurnoverOption" ng-model="HistoricalInfo.FinalTurnoverOption" placeholder="" ng-disabled="true">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-6">
                                                                <div class="form-group row mb-0">
                                                                    <label for="FinalTurnoverRemarks" class="col-sm-5 col-form-label font-weight-normal">Remarks</label>
                                                                    <div class="col-sm-7">
                                                                        <textarea class="form-control form-control-sm text-xs font-italic" id="FinalTurnoverRemarks" rows="2" name="FinalTurnoverRemarks" ng-model="HistoricalInfo.FinalTurnoverRemarks" placeholder="" ng-disabled="true"></textarea>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <p class="font-weight-bold mt-2 mb-2"><u>Details: Turnover Status</u></p>
                                                        <div class="row">
                                                            <div class="col-6">
                                                                <div class="form-group row mb-0 has-feedback" show-errors>
                                                                    <label for="TurnoverStatus" class="col-sm-5 col-form-label font-weight-normal">Turnover Status <span class="text-danger font-weight-bold">*</span></label>
                                                                    <div class="col-sm-7">
                                                                        <select class="form-control form-control-sm" id="TurnoverStatus" name="TurnoverStatus" ng-model="UnitHistorical.TurnoverStatus" ng-change="changeTOStatus()" ng-disabled="disableButton || TASFields.TurnoverStatus" ng-required="true">
                                                                            <option value="">Select Please ...</option>
                                                                            <option ng-repeat="ts in TurnoverStatus" value="{{ts.Name}}">{{ts.Name}} {{(ts.Description !== '')? ' - ' + ts.Description : ts.Description}}</option>
                                                                        </select>
                                                                        <div style="color:red" class="invalid-feedback mt-0 mb-1" ng-if="toacceptstatusForm.TurnoverStatus.$error.required">You can't leave this empty.</div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-6">
                                                                <div class="form-group row mb-0 has-feedback" show-errors>
                                                                    <label for="PunchlistCategory" class="col-sm-5 col-form-label font-weight-normal">Punchlist Category <span class="text-danger font-weight-bold" ng-show="!TASFields.PunchlistCategory">*</span></label>
                                                                    <div class="col-sm-7">
                                                                        <select class="form-control form-control-sm" id="PunchlistCategory" name="PunchlistCategory" ng-model="UnitHistorical.PunchlistCategory" 
                                                                                ng-disabled="disableButton || TASFields.PunchlistCategory"
                                                                                ng-required="(UnitHistorical.TurnoverStatus === 'AWP' || UnitHistorical.TurnoverStatus === 'NAPL')">
                                                                            <option value="">Select Please ...</option>
                                                                            <option ng-repeat="pc in PunchlistCategory" value="{{pc.Name}}">{{pc.Name}}</option>
                                                                        </select>
                                                                        <div style="color:red" class="invalid-feedback mt-0 mb-1" ng-if="(!TASFields.PunchlistCategory)? toacceptstatusForm.PunchlistCategory.$error.required : ''">You can't leave this empty.</div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-6">
                                                                <div class="form-group row mb-0 has-feedback" show-errors>
                                                                    <label for="PunchlistItem" class="col-sm-5 col-form-label font-weight-normal">Punchlist Items <span class="text-danger font-weight-bold" ng-show="!TASFields.PunchlistItems">*</span></label>
                                                                    <div class="col-sm-7">
                                                                        <input type="text" class="form-control form-control-sm" id="PunchlistItem" name="PunchlistItem" ng-model="UnitHistorical.PunchlistItem" placeholder=""
                                                                               ng-disabled="disableButton || TASFields.PunchlistItems"
                                                                               ng-required="!TASFields.PunchlistItems">
                                                                        <div style="color:red" class="invalid-feedback mt-0 mb-1" ng-if="(!TASFields.PunchlistItems)? toacceptstatusForm.PunchlistItem.$error.required : ''">You can't leave this empty.</div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-6">
                                                                <div class="form-group row mb-0 has-feedback" show-errors>
                                                                    <label for="OtherIssues" class="col-sm-5 col-form-label font-weight-normal">Other Issues <span class="text-danger font-weight-bold" ng-show="(UnitHistorical.TurnoverStatus === 'NAOI')">*</span></label>
                                                                    <div class="col-sm-7">
                                                                        <input type="text" class="form-control form-control-sm" id="OtherIssues" name="OtherIssues" ng-model="UnitHistorical.OtherIssues" placeholder=""
                                                                               ng-disabled="disableButton || TASFields.OtherIssues"
                                                                               ng-required="(UnitHistorical.TurnoverStatus === 'NAOI')">
                                                                        <div style="color:red" class="invalid-feedback mt-0 mb-1" ng-if="(UnitHistorical.TurnoverStatus === 'NAOI')? toacceptstatusForm.OtherIssues.$error.required : ''">You can't leave this empty.</div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-6">
                                                                <div class="form-group row mb-0">
                                                                    <label for="TSRemarks" class="col-sm-5 col-form-label font-weight-normal">Remarks</label>
                                                                    <div class="col-sm-7">
                                                                        <textarea class="form-control form-control-sm" id="TSRemarks" rows="2" name="TSRemarks" ng-model="UnitHistorical.TSRemarks" placeholder="" ng-disabled="disableButton || TASFields.TSRemarks"></textarea>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-6">
                                                                <div class="form-group row mb-0">
                                                                    <label for="TSAttachment" class="col-sm-5 col-form-label font-weight-normal">Attachment</label>
                                                                    <div class="col-sm-7">
                                                                        <input type="text" class="form-control form-control-sm" id="TSAttachment" name="TSAttachment" ng-model="UnitHistorical.TSAttachment" placeholder="" ng-disabled="true">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <p class="font-weight-bold mt-2 mb-2"><u>Details: Acceptance Details</u></p>
                                                        <div class="row">
                                                            <div class="col-6">
                                                                <div class="form-group row mb-0 has-feedback" show-errors>
                                                                    <label for="UnitAcceptanceDate" class="col-sm-5 col-form-label font-weight-normal">Unit Acceptance Date</label>
                                                                    <div class="col-sm-7">
                                                                        <div class="input-group mb-0">
                                                                            <input type="text" name="UnitAcceptanceDate" id="UnitAcceptanceDate" class="form-control form-control-sm" onkeydown="return false" @*onkeydown="return false"*@
                                                                                   uib-datepicker-popup="{{dateFormat}}" ng-model="UnitHistorical.UnitAcceptanceDate" is-open="popup1.opened" datepicker-options="dateOptionsDefault"
                                                                                   ng-click="open1()" ng-disabled="disableButton || TASFields.UnitAcceptanceDate" placeholder="mm/dd/yyyy" close-text="Close" />
                                                                            <div class="input-group-append">
                                                                                <div class="input-group-text" ng-click="open1()">
                                                                                    <span class="fas fa-calendar-alt"></span>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div style="color:red" class="invalid-feedback" ng-if="!toacceptstatusForm.UnitAcceptanceDate.$valid">Date format should be (MM/dd/yyyy)</div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-6">
                                                                <div class="form-group row mb-0 has-feedback" show-errors>
                                                                    <label for="KeyTransmittalDate" class="col-sm-5 col-form-label font-weight-normal">Key Transmittal Date</label>
                                                                    <div class="col-sm-7">
                                                                        <div class="input-group mb-0">
                                                                            <input type="text" name="KeyTransmittalDate" id="KeyTransmittalDate" class="form-control form-control-sm" onkeydown="return false" @*onkeydown="return false"*@
                                                                                   uib-datepicker-popup="{{dateFormat}}" ng-model="UnitHistorical.KeyTransmittalDate" is-open="popup2.opened" datepicker-options="dateOptionsCurrent"
                                                                                   ng-click="open2()" ng-disabled="disableButton || TASFields.KeyTransmittalDate || !((UnitHistorical.UnitAcceptanceDate | date : 'yy').length > 0)" placeholder="mm/dd/yyyy" close-text="Close" />
                                                                            <div class="input-group-append">
