
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content" ng-controller="SystemParameter" ng-init="init('@ViewBag.PageUrl')">
    <div class="container-fluid" ng-init="GetSystemParameterData()">
        <div class="row">
            <div class="col-12">
                <div class="loading" ng-if="loading">Loading &#8230;</div>
                <div class="card">
                    <div class="card-header card-gray card-outline">
                        <div class="card-tools">
                            <button type="button" class="btn btn-tool" data-card-widget="collapse">
                                <i class="fas fa-minus"></i>
                            </button>
                            <button type="button" class="btn btn-tool" data-card-widget="remove">
                                <i class="fas fa-times"></i>
                            </button>
                        </div>
                    </div>
                    <div class="card-body">
                        <form name="systemParameterForm" novalidate role="form">
                            <div class="row mb-3">
                                <div class="col-sm-12 col-md-6">
                                    <p class="font-italic"><strong>Note:</strong> Essential information is marked with an asterisk (<span class="text-danger font-weight-bold">*</span>)</p>
                                </div>
                                <div class="col-sm-12 col-md-6">
                                    <button type="button" class="btn btn-secondary btn-sm float-right mr-1" ng-click="GetSystemParameterData()">
                                        <i class="fas fa-ban"></i> Cancel
                                    </button>
                                    <button type="button" class="btn btn-info btn-sm float-right mr-1" ng-if="Ctrl.wEdit" ng-click="UpdateSystemParameter(settings)">
                                        <i class="fas fa-save"></i> Update
                                    </button>
                                </div>
                            </div>
                            <div class="row mb-1">
                                <div class="col-sm-12 col-md-5">
                                    <div class="form-group has-feedback" show-errors>
                                        <label for="Title" class="font-weight-normal">Configuration Name <span class="text-danger font-weight-bold">*</span></label>
                                        <input type="text" class="form-control form-control-sm" id="Title" name="Title" ng-model="settings.Title" ng-required="true" placeholder="Title">
                                        <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.Title.$error.required">You can't leave this empty.</div>
                                    </div>
                                </div>
                                <div class="col-sm-12 col-md-3">
                                    <div class="form-group has-feedback" show-errors>
                                        <label for="BsHrFrm" class="font-weight-normal">Business Hours <span class="text-danger font-weight-bold">*</span></label>
                                        <div class="row">
                                            <div class="col-6">
                                                <input type="time" class="form-control form-control-sm" id="BsHrFrm" name="BsHrFrm" ng-model="settings.BsHrFrm" ng-required="true" placeholder="HH:mm:ss">
                                                <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.BsHrFrm.$error.required">You can't leave this empty.</div>
                                            </div>
                                            <div class="col-6 pl-0">
                                                <input type="time" class="form-control form-control-sm" id="BsHrTo" name="BsHrTo" ng-model="settings.BsHrTo" ng-required="true" placeholder="HH:mm:ss">
                                                <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.BsHrTo.$error.required">You can't leave this empty.</div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-12 col-md-2">
                                    <div class="form-group has-feedback" show-errors>
                                        <label for="Applicability" class="font-weight-normal">Applicability <span class="text-danger font-weight-bold">*</span></label>
                                        <select class="form-control form-control-sm" id="Applicability" name="Applicability" ng-model="settings.Applicability" ng-required="true" ng-disabled="true">
                                            <option value="">Please Select ...</option>
                                            <option value="01">Across All</option>
                                            <option value="02" ng-selected="true">New Transactions Only</option>
                                        </select>
                                        <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.Applicability.$error.required">You can't leave this empty.</div>
                                    </div>
                                </div>
                                <div class="col-sm-12 col-md-2">
                                    <div class="form-group">
                                        <label for="published" class="font-weight-normal">Published</label>
                                        <select class="form-control form-control-sm" id="published" name="published" ng-model="settings.Published" ng-required="true" ng-disabled="true">
                                            <option value="True" ng-selected="true">Active</option>
                                            <option value="False">Inactive</option>
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div class="card card-gray card-tabs mb-0">
                                <div class="card-header p-0 pt-1">
                                    <ul class="nav nav-tabs" id="custom-content-below-tab" role="tablist">
                                        <li class="nav-item">
                                            <a class="nav-link active" id="custom-notice-tab1" data-toggle="pill" href="#custom-tab1" role="tab" aria-controls="custom-tab1" aria-selected="true">Unit Qualification &amp; Turnover Management</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="custom-schedule-tab2" data-toggle="pill" href="#custom-tab2" role="tab" aria-controls="custom-tab2" aria-selected="false">Unit Inspection &amp; Acceptance Management</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="custom-acceptance-tab3" data-toggle="pill" href="#custom-tab3" role="tab" aria-controls="custom-tab3" aria-selected="false">Titling Status</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" id="custom-acceptance-tab4" data-toggle="pill" href="#custom-tab4" role="tab" aria-controls="custom-tab4" aria-selected="false">Electric Meter</a>
                                        </li>
                                    </ul>
                                </div>
                                <div class="card-body">
                                    <div class="tab-content" id="custom-content-below-tabContent">
                                        <div class="tab-pane fade show active" id="custom-tab1" role="tabpanel" aria-labelledby="custom-tab1">
                                            <p class="mb-3 text-md"><strong><em>Notice of Unit Turnover</em></strong></p>
                                            <div class="row">
                                                <div class="col-sm-12 col-md-6">
                                                    <p class="mb-1"><strong>Turnover Date Max Calendar Date:</strong> <span class="text-xs font-italic">(day(s) from Date of Email Notice Sent)</span></p>
                                                    <div class="form-group row mb-1 has-feedback" show-errors>
                                                        <label for="TurnoverMaxDays" class="col-sm-4 col-form-label font-weight-normal">Nos of day(s) <span class="text-danger font-weight-bold">*</span></label>
                                                        <div class="col-sm-6">
                                                            <div class="row">
                                                                <div class="col-5">
                                                                    <input type="number" class="form-control form-control-sm" id="TurnoverMaxDays" name="TurnoverMaxDays" ng-model="settings.TurnoverMaxDays" ng-required="true" placeholder="0">
                                                                    <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.TurnoverMaxDays.$error.required">You can't leave this empty.</div>
                                                                </div>
                                                                <div class="col-7 pl-0">
                                                                    <select class="form-control form-control-sm" id="TurnoverMaxDaysCT" name="TurnoverMaxDaysCT" ng-model="settings.TurnoverMaxDaysCT">
                                                                        <option value="WD">Working Days</option>
                                                                        <option value="CD">Calendar Days</option>
                                                                    </select>
                                                                    <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.TurnoverMaxDaysCT.$error.required">You can't leave this empty.</div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div> <!-- .row -->

                                            <div class="row">
                                                <div class="col-sm-12 col-md-6">
                                                    <div class="form-check mb-2 mt-2">
                                                        <input type="checkbox" id="NoticeSendRecipient1" name="NoticeSendRecipient1" class="form-check-input" ng-model="settings.NoticeSendRecipient1" ng-checked="(settings.NoticeSendRecipient1)? true : false" />
                                                        <label class="form-check-label"><u><strong>Notice Turnover Posting:</strong> (1st Notification)</u></label>
                                                    </div>

                                                    <div class="form-group row mb-1 has-feedback" show-errors>
                                                        <label for="NoticeNosDays1" class="col-sm-4 col-form-label font-weight-normal">Nos of day(s) <span class="text-danger font-weight-bold">*</span></label>
                                                        <div class="col-sm-6">
                                                            <div class="row">
                                                                <div class="col-5">
                                                                    <input type="number" class="form-control form-control-sm" id="NoticeNosDays1" name="NoticeNosDays1" ng-model="settings.NoticeNosDays1" ng-required="settings.NoticeSendRecipient1? 'true' : 'false'" ng-disabled="!settings.NoticeSendRecipient1" placeholder="0">
                                                                    <div style="color:red" class="invalid-feedback" ng-if="(settings.NoticeSendRecipient1)? systemParameterForm.NoticeNosDays1.$error.required : ''">You can't leave this empty.</div>
                                                                </div>
                                                                <div class="col-7 pl-0">
                                                                    <select class="form-control form-control-sm" id="NoticeCT1" name="NoticeCT1" ng-model="settings.NoticeCT1" ng-required="settings.NoticeSendRecipient1? 'true' : 'false'" ng-disabled="!settings.NoticeSendRecipient1">
                                                                        <option value="WD">Working Days</option>
                                                                        <option value="CD">Calendar Days</option>
                                                                    </select>
                                                                    <div style="color:red" class="invalid-feedback" ng-if="(settings.NoticeSendRecipient1)? systemParameterForm.NoticeCT1.$error.required : ''">You can't leave this empty.</div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="form-group row mb-1 has-feedback" show-errors>
                                                        <label for="NoticeRecipientEmail1" class="col-sm-4 col-form-label font-weight-normal">Email Recipient <span class="text-danger font-weight-bold">*</span> <small>(Multiple emails should be separated by comma)</small></label>
                                                        <div class="col-sm-6">
                                                            <textarea class="form-control form-control-sm" id="NoticeRecipientEmail1" name="NoticeRecipientEmail1" ng-model="settings.NoticeRecipientEmail1" ng-required="settings.NoticeSendRecipient1? 'true' : 'false'" ng-disabled="!settings.NoticeSendRecipient1" rows="3" placeholder="Email"></textarea>
                                                            <div style="color:red" class="invalid-feedback" ng-if="(settings.NoticeSendRecipient1)? systemParameterForm.NoticeRecipientEmail1.$error.required : ''">You can't leave this empty.</div>
                                                        </div>
                                                    </div>

                                                    <div class="form-group row mb-1 has-feedback" show-errors>
                                                        <label for="NoticeTemplateID1" class="col-sm-4 col-form-label font-weight-normal">Email Template <span class="text-danger font-weight-bold">*</span></label>
                                                        <div class="col-sm-6">
                                                            <select class="form-control form-control-sm" id="NoticeTemplateID1" name="NoticeTemplateID1" ng-model="settings.NoticeTemplateID1" ng-required="settings.NoticeSendRecipient1? 'true' : 'false'" ng-disabled="!settings.NoticeSendRecipient1">
                                                                <option value="">Select Please ...</option>
                                                                <option ng-repeat="et in EmailTemplates" value="{{et.Id}}">{{et.Name}}</option>
                                                            </select>
                                                            <div style="color:red" class="invalid-feedback" ng-if="(settings.NoticeSendRecipient1)? systemParameterForm.NoticeTemplateID1.$error.required : ''">You can't leave this empty.</div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-sm-12 col-md-6">
                                                    <div class="form-check mb-2 mt-2">
                                                        <input type="checkbox" id="NoticeSendRecipient2" name="NoticeSendRecipient2" class="form-check-input" ng-model="settings.NoticeSendRecipient2" ng-checked="(settings.NoticeSendRecipient2)? true : false" />
                                                        <label class="form-check-label"><u><strong>Notice Turnover Posting:</strong> (2nd Notification)</u></label>
                                                    </div>

                                                    <div class="form-group row mb-1 has-feedback" show-errors>
                                                        <label for="NoticeNosDays2" class="col-sm-4 col-form-label font-weight-normal">Nos of day(s) <span class="text-danger font-weight-bold">*</span></label>
                                                        <div class="col-sm-6">
                                                            <div class="row">
                                                                <div class="col-5">
                                                                    <input type="number" class="form-control form-control-sm" id="NoticeNosDays2" name="NoticeNosDays2" ng-model="settings.NoticeNosDays2" ng-required="settings.NoticeSendRecipient2? 'true' : 'false'" ng-disabled="!settings.NoticeSendRecipient2" placeholder="0">
                                                                    <div style="color:red" class="invalid-feedback" ng-if="(settings.NoticeSendRecipient2)? systemParameterForm.NoticeNosDays2.$error.required : ''">You can't leave this empty.</div>
                                                                </div>
                                                                <div class="col-7 pl-0">
                                                                    <select class="form-control form-control-sm" id="NoticeCT2" name="NoticeCT2" ng-model="settings.NoticeCT2" ng-required="settings.NoticeSendRecipient2? 'true' : 'false'" ng-disabled="!settings.NoticeSendRecipient2">
                                                                        <option value="WD">Working Days</option>
                                                                        <option value="CD">Calendar Days</option>
                                                                    </select>
                                                                    <div style="color:red" class="invalid-feedback" ng-if="(settings.NoticeSendRecipient2)? systemParameterForm.NoticeCT2.$error.required : ''">You can't leave this empty.</div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="form-group row mb-1 has-feedback" show-errors>
                                                        <label for="NoticeRecipientEmail2" class="col-sm-4 col-form-label font-weight-normal">Email Recipient <span class="text-danger font-weight-bold">*</span> <small>(Multiple emails should be separated by comma)</small></label>
                                                        <div class="col-sm-6">
                                                            <textarea class="form-control form-control-sm" id="NoticeRecipientEmail2" name="NoticeRecipientEmail2" ng-model="settings.NoticeRecipientEmail2" ng-required="settings.NoticeSendRecipient2? 'true' : 'false'" ng-disabled="!settings.NoticeSendRecipient2" rows="3" placeholder="Email"></textarea>
                                                            <div style="color:red" class="invalid-feedback" ng-if="(settings.NoticeSendRecipient2)? systemParameterForm.NoticeRecipientEmail2.$error.required : ''">You can't leave this empty.</div>
                                                        </div>
                                                    </div>

                                                    <div class="form-group row mb-1 has-feedback" show-errors>
                                                        <label for="NoticeTemplateID2" class="col-sm-4 col-form-label font-weight-normal">Email Template <span class="text-danger font-weight-bold">*</span></label>
                                                        <div class="col-sm-6">
                                                            <select class="form-control form-control-sm" id="NoticeTemplateID2" name="NoticeTemplateID2" ng-model="settings.NoticeTemplateID2" ng-required="settings.NoticeSendRecipient2? 'true' : 'false'" ng-disabled="!settings.NoticeSendRecipient2">
                                                                <option value="">Select Please ...</option>
                                                                <option ng-repeat="et in EmailTemplates" value="{{et.Id}}">{{et.Name}}</option>
                                                            </select>
                                                            <div style="color:red" class="invalid-feedback" ng-if="(settings.NoticeSendRecipient2)? systemParameterForm.NoticeTemplateID2.$error.required : ''">You can't leave this empty.</div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div> <!-- .row -->

                                            <hr />
                                            <p class="mb-3 text-md"><strong><em>Turnover Schedule &amp; Option</em></strong></p>
                                            <div class="row">
                                                <div class="col-sm-12 col-md-6">
                                                    <p class="mb-1"><strong>Turnover Date Max Calendar Date:</strong> <span class="text-xs font-italic">(day(s) from Date of Email Notice Sent)</span></p>
                                                    <div class="form-group row mb-3 has-feedback" show-errors>
                                                        <label for="TurnoverMaxDays2" class="col-sm-4 col-form-label font-weight-normal">Nos of day(s) <span class="text-danger font-weight-bold">*</span></label>
                                                        <div class="col-sm-6">
                                                            <div class="row">
                                                                <div class="col-5">
                                                                    <input type="number" class="form-control form-control-sm" id="TurnoverMaxDays2" name="TurnoverMaxDays2" ng-model="settings.TurnoverMaxDays2" ng-required="true" placeholder="0">
                                                                    <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.TurnoverMaxDays2.$error.required">You can't leave this empty.</div>
                                                                </div>
                                                                <div class="col-7 pl-0">
                                                                    <select class="form-control form-control-sm" id="TurnoverMaxDays2CT" name="TurnoverMaxDays2CT" ng-model="settings.TurnoverMaxDays2CT">
                                                                        <option value="WD">Working Days</option>
                                                                        <option value="CD">Calendar Days</option>
                                                                    </select>
                                                                    <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.TurnoverMaxDays2CT.$error.required">You can't leave this empty.</div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-sm-12 col-md-6">
                                                    <p class="mb-2">
                                                        If after a given period, there is NO CONFIRMATION or NO RE-SCHEDULE/CHANGES on the Turnover
                                                        Schedule and Turnover Option on the system, the following paramater will be applied:
                                                    </p>
                                                    <p class="mb-1"><strong>NO confirmation</strong> of 1st Turnover Schedule &amp; Turnover Option</p>
                                                    <div class="form-group row mb-1 has-feedback" show-errors>
                                                        <label for="ReschedMaxDays1" class="col-sm-4 col-form-label font-weight-normal">Nos of day(s) <span class="text-danger font-weight-bold">*</span></label>
                                                        <div class="col-sm-6">
                                                            <div class="row">
                                                                <div class="col-5">
                                                                    <input type="number" class="form-control form-control-sm" id="ReschedMaxDays1" name="ReschedMaxDays1" ng-model="settings.ReschedMaxDays1" ng-required="true" placeholder="0">
                                                                    <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.ReschedMaxDays1.$error.required">You can't leave this empty.</div>
                                                                </div>
                                                                <div class="col-7 pl-0">
                                                                    <select class="form-control form-control-sm" id="ReschedMaxDays1CT" name="ReschedMaxDays1CT" ng-model="settings.ReschedMaxDays1CT">
                                                                        <option value="WD">Working Days</option>
                                                                        <option value="CD">Calendar Days</option>
                                                                    </select>
                                                                    <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.ReschedMaxDays1CT.$error.required">You can't leave this empty.</div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <p class="mb-1"><strong>NO reschedule or change</strong> on the Turnover Schedule and/or Turnover Option</p>
                                                    <div class="form-group row mb-1 has-feedback" show-errors>
                                                        <label for="ReschedMaxDays2" class="col-sm-4 col-form-label font-weight-normal">Nos of day(s) <span class="text-danger font-weight-bold">*</span></label>
                                                        <div class="col-sm-6">
                                                            <div class="row">
                                                                <div class="col-5">
                                                                    <input type="number" class="form-control form-control-sm" id="ReschedMaxDays2" name="ReschedMaxDays2" ng-model="settings.ReschedMaxDays2" ng-required="true" placeholder="0">
                                                                    <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.ReschedMaxDays2.$error.required">You can't leave this empty.</div>
                                                                </div>
                                                                <div class="col-7 pl-0">
                                                                    <select class="form-control form-control-sm" id="ReschedMaxDays2CT" name="ReschedMaxDays2CT" ng-model="settings.ReschedMaxDays2CT">
                                                                        <option value="WD">Working Days</option>
                                                                        <option value="CD">Calendar Days</option>
                                                                    </select>
                                                                    <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.ReschedMaxDays2CT.$error.required">You can't leave this empty.</div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div> <!-- .row -->
                                        </div> <!-- /.custom-tab1 -->

                                        <div class="tab-pane fade" id="custom-tab2" role="tabpanel" aria-labelledby="custom-tab2">
                                            <p class="mb-3 text-md"><strong><em>Turnover &amp; Acceptance Status</em></strong></p>
                                            <div class="row">
                                                <div class="col-sm-12 col-md-6">
                                                    <p class="mb-1"><strong>Turnover Status Tagging TAT:</strong> <span class="text-xs font-italic">(day(s) from final/confirmed turnover schedule of unit owner)</span></p>
                                                    <div class="form-group row mb-1 has-feedback" show-errors>
                                                        <label for="TurnoverStatusTAT" class="col-sm-4 col-form-label font-weight-normal">Nos of day(s) <span class="text-danger font-weight-bold">*</span></label>
                                                        <div class="col-sm-6">
                                                            <div class="row">
                                                                <div class="col-5">
                                                                    <input type="number" class="form-control form-control-sm" id="TurnoverStatusTAT" name="TurnoverStatusTAT" ng-model="settings.TurnoverStatusTAT" ng-required="true" placeholder="0">
                                                                    <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.TurnoverStatusTAT.$error.required">You can't leave this empty.</div>
                                                                </div>
                                                                <div class="col-7 pl-0">
                                                                    <select class="form-control form-control-sm" id="TurnoverStatusTATCT" name="TurnoverStatusTATCT" ng-model="settings.TurnoverStatusTATCT">
                                                                        <option value="WD">Working Days</option>
                                                                        <option value="CD">Calendar Days</option>
                                                                    </select>
                                                                    <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.TurnoverStatusTATCT.$error.required">You can't leave this empty.</div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <p class="mb-1"><strong>Punchlist Posting System-Computed :</strong> <span class="text-xs font-italic">(day(s) from the posting of Punchlist Items)</span></p>
                                                    <div class="form-group row mb-1 has-feedback" show-errors>
                                                        <label for="PunchlistDateTAT" class="col-sm-4 col-form-label font-weight-normal">Nos of day(s) <span class="text-danger font-weight-bold">*</span></label>
                                                        <div class="col-sm-6">
                                                            <div class="row">
                                                                <div class="col-5">
                                                                    <input type="number" class="form-control form-control-sm" id="PunchlistDateTAT" name="PunchlistDateTAT" ng-model="settings.PunchlistDateTAT" ng-required="true" placeholder="0">
                                                                    <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.PunchlistDateTAT.$error.required">You can't leave this empty.</div>
                                                                </div>
                                                                <div class="col-7 pl-0">
                                                                    <select class="form-control form-control-sm" id="PunchlistDateTATCT" name="PunchlistDateTATCT" ng-model="settings.PunchlistDateTATCT">
                                                                        <option value="WD">Working Days</option>
                                                                        <option value="CD">Calendar Days</option>
                                                                    </select>
                                                                    <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.PunchlistDateTATCT.$error.required">You can't leave this empty.</div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <p class="mb-1"><strong>Re-inspection Date TAT:</strong> <span class="text-xs font-italic">(day(s) from Date of Re-inspection)</span></p>
                                                    <div class="form-group row mb-1 has-feedback" show-errors>
                                                        <label for="ReinspectionDateTAT" class="col-sm-4 col-form-label font-weight-normal">Nos of day(s) <span class="text-danger font-weight-bold">*</span></label>
                                                        <div class="col-sm-6">
                                                            <div class="row">
                                                                <div class="col-5">
                                                                    <input type="number" class="form-control form-control-sm" id="ReinspectionDateTAT" name="ReinspectionDateTAT" ng-model="settings.ReinspectionDateTAT" ng-required="true" placeholder="0">
                                                                    <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.ReinspectionDateTAT.$error.required">You can't leave this empty.</div>
                                                                </div>
                                                                <div class="col-7 pl-0">
                                                                    <select class="form-control form-control-sm" id="ReinspectionDateTATCT" name="ReinspectionDateTATCT" ng-model="settings.ReinspectionDateTATCT">
                                                                        <option value="WD">Working Days</option>
                                                                        <option value="CD">Calendar Days</option>
                                                                    </select>
                                                                    <div style="color:red" class="invalid-feedback" ng-if="systemParameterForm.ReinspectionDateTATCT.$error.required">You can't leave this empty.</div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

